Time":"3min","abstract":[{"type":"text","text":"We need to make sure you have everything to start."}],"identifier":"doc:\/\/ch.ti8m.idcheck.library\/tutorials\/IDCheckLib\/Getting-Started","kind":"project","type":"topic","url":"\/tutorials\/idchecklib\/getting-started"},"IDCheck_task_processing.swift":{"syntax":"","content":["func process(_ taskWrapper: IDCheckNextTask) {","    guard let nextAvailableTask = taskWrapper.task else {","        statusLabel.text = \"The next task wrapper that was returned doesn't hold any information about the actual task that's needed for processing :(\"","        return","    }","","    switch nextAvailableTask {","    case let .documentVerify(documentVerifyTask):","        present(","            DocumentVerificationViewController(","                processAPI: myProcessAPI,","                task: documentVerifyTask,","                delegate: self","            ),","            animated: true","        ) { [weak self] in","            self?.statusLabel.text = \"Processing the document verification task...\"","        }","    case let .documentCorrection(documentCorrectionTask):","        present(","            DocumentCorrectionViewController(","                processAPI: myProcessAPI,","                task: documentCorrectionTask,","                delegate: self","            ),","            animated: true","        ) { [weak self] in","            self?.statusLabel.text = \"Processing the document correction task...\"","        }","    case let .emrtdVerify(emrtdVerifyTask):","        present(","            EMRTDVerificationViewController(","                processAPI: myProcessAPI,","                task: emrtdVerifyTask,","                delegate: self","            ),","            animated: true","        ) { [weak self] in","            self?.statusLabel.text = \"Processing the EMRTD verification task...\"","        }","    case let .livenessVerify(livenessVerifyTask):","        if livenessVerifyTask.taskType == \"TaskLivenessVerifyV3\" {","            present(","                SelfieVerificationViewController(","                    processAPI: myProcessAPI,","                    task: livenessVerifyTask,","                    delegate: self","                ),","                animated: true","            ) { [weak self] in","                self?.statusLabel.text = \"Processing the liveness verification task...\"","            }","        } else {","            guard let requiredHeadPoses = livenessVerifyTask.requiredHeadPoses else {","                \/\/ handle no requiried head poses in payload","                statusLabel.text = \"No requiredHeadPoses on the liveness task: \\(livenessVerifyTask)\"","                return","            }","            present(","                SelfieVerificationLegacyViewController(","                    processAPI: myProcessAPI,","                    task: livenessVerifyTask,","                    requiredHeadPoses: requiredHeadPoses,","                    delegate: self","                ),","                animated: true","            ) { [weak self] in","                self?.statusLabel.text = \"Processing the liveness verification task...\"","            }","        }","    case let .addressVerify(addressVerifyTask):","        present(","            AddressVerificationViewController(","                processAPI: myProcessAPI,","                task: addressVerifyTask,","                delegate: self","            ),","            animated: true","        ) { [weak self] in","            self?.statusLabel.text = \"Processing the address verification task...\"","        }","    default:","        statusLabel.text = \"Unknown next task type: \\(nextAvailableTask.taskType)\"","    }","}"],"fileName":"IDCheckProcessAPI task processing","identifier":"IDCheck_task_processing.swift","type":"file","fileType":"","highlights":[]},"doc://ch.ti8m.idcheck.library/tutorials/IDCheckLib/Integration-Code#Using-the-SDK":{"role":"pseudoSymbol","tit